LIB     = ../../libgfsio_4.a
INCMOD  = ../../incmod/gfsio_4
FC      = $(FCS)
FFLAGS  = -traceback -g -xHOST -convert big_endian -assume byterecl  -I$(INCMOD) -FR
#FFLAGS = -g -C -qextchk -qflttrap -qnosave -O3 -q free=f90 -I$(INCMOD)
AR      = ar
ARFLAGS = -rv 

$(LIB): $(LIB)(gfsio_module.o) 
.f.a:
	$(FC) -c $(FFLAGS) $<
	$(AR) $(ARFLAGS) $@ $*.o
	rm -f $*.o
	mv $*.mod $(INCMOD)

